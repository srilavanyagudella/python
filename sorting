def sort_key(char):
    if char.islower():
        # Lowercase letters come first (Priority 0)
        return (0, char)
    elif char.isupper():
        # Uppercase letters come second (Priority 1)
        return (1, char)
    elif char.isdigit():
        val = int(char)
        if val % 2 != 0:
            # Odd digits come third (Priority 2)
            return (2, char)
        else:
            # Even digits come last (Priority 3)
            return (3, char)

s = input()
# Apply the custom key to the sorted function
sorted_s = sorted(s, key=sort_key)

# Join the list back into a string and print
print("".join(sorted_s))
